{
  "name": "greenhouse-auto-apply-monorepo",
  "version": "1.0.0",
  "description": "Monorepo for Greenhouse auto-apply bot",
  "private": true,
  "workspaces": [
    "cdk",
    "frontend"
  ],
  "scripts": {
    "build": "npm run build --workspaces",
    "dev:backend": "npm run dev --workspace=cdk",
    "dev:frontend": "npm run dev --workspace=frontend",
    "run": "npm run run --workspace=cdk",
    "start": "npm run start --workspace=cdk",
    "test": "npm run test --workspace=cdk",
    "playwright:install": "npm run playwright:install --workspace=cdk"
  },
  "keywords": [
    "greenhouse",
    "job-application",
    "automation",
    "bot",
    "playwright",
    "monorepo"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/aws-lambda": "^8.10.157",
    "@tsconfig/recommended": "^1.0.8"
  }
}
